{
  "manifest_version": 3,
  "name": "Resume Builder",
  "version": "1.0",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  }, 
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["popup.js"],
    "run_at": "document_idle"
  }
]

}